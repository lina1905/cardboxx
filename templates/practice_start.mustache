<div class="doughnut-chart" style="margin-top: 30px; background: conic-gradient(#BF514C {{duecardcountpercentage}}%, #71A87F {{duecardcountpercentage}}% 100%);">
    <div class="doughnut-chart-text">
        {{# str }} chartpractice, cardboxx {{/str}} <br>
        {{duecardcount}}/{{cardcount}}
    </div>
</div>

<button id="cardboxx-see-options" type="button" class="btn btn-primary" data-toggle="modal" data-target="#cardboxxPracticeSettings" style="margin-top: 40px;">
    {{# str }} options, cardboxx {{/ str }}
</button>



<div class="modal" id="cardboxxPracticeSettings">
    {{> mod_cardboxx/options }}
</div>

