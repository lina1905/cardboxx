<div class="doughnut-chart" style="margin-top: 30px; background: conic-gradient(#BF514C {{duecardcountpercentage}}%, #71A87F {{duecardcountpercentage}}% 100%);">
    <div class="doughnut-chart-text">
        {{# str }} chartpractice, cardbox {{/str}} <br>
        {{duecardcount}}/{{cardcount}}
    </div>
</div>

<button id="cardbox-see-options" type="button" class="btn btn-primary" data-toggle="modal" data-target="#cardboxPracticeSettings" style="margin-top: 40px;">
    {{# str }} options, cardbox {{/ str }}
</button>



<div class="modal" id="cardboxPracticeSettings">
    {{> mod_cardbox/options }}
</div>

