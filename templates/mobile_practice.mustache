{{=<% %>=}}

<ion-content>
    <div id="cardboxx-practice" class="container-fluid cardboxx-practice">

        <div id="cardboxx-practice-replacable">
            <%# case1 %>
            <ng-container *ngTemplateOutlet="mobile_practice_question_selfcheck"></ng-container>
            <%/ case1 %>

            <%# case2 %>
            <ng-container *ngTemplateOutlet="mobile_practice_answer_selfcheck"></ng-container>
            <%/ case2 %>
        </div>

        <div id="cardboxx-practice-feedback-collection" class="row" style="display: none">
            <div class="col-xs-1 col-md-3 col-lg-4 col-xl-4"></div>
            <div class="col-xs-10 col-md-6 col-lg-4 col-xl-4">
                <canvas id="cardboxx-practice-feedback" width="100" height="100"></canvas>
            </div>
            <div class="col-xs-1 col-md-3 col-lg-4 col-xl-4"></div>
            <ion-button id="back-to-start" expand="full" style="margin-top: 20px">
                {{ 'continuepracticebutton' | translate }}
            </ion-button>
        </div>
    </div>
</ion-content>